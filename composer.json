{
    "name": "dlds/yii2-summernote",
    "description": "Yii2 Intercooler extensions",
    "keywords": ["yii2", "summernote"],
    "type": "yii2-extension",
    "license": "BSD",
    "support": {
        "issues": "https://github.com/dlds/yii2-summernote/issues",
        "wiki": "https://github.com/dlds/yii2-summernote/wiki",
        "source": "https://github.com/dlds/yii2-summernote"
    },
    "authors": [
        {
            "name": "Jiri Svoboda",
            "email": "jiri.svoboda@dlds.cz"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2",
        "bower-asset/font-awesome": "4.*@stable",
        "bower-asset/codemirror": "5.*@stable",
        "bower-asset/summernote": "0.*@stable"
    },
    "autoload": {
        "psr-4": {
            "dlds\\summernote\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
